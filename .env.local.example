# Gemini API Key from Google AI Studio
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Key (For Embeddings)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Supabase Configuration (For RAG & Auth)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Lemon Squeezy (For Monetization)
LEMON_SQUEEZY_API_KEY=your_api_key
LEMON_SQUEEZY_WEBHOOK_SECRET=your_webhook_secret

# FingerprintJS (For Rate Limiting)
FINGERPRINTJS_API_KEY=your_api_key

# VL Embedding Service (B2 Rollout - Qwen3-VL-Embedding-8B)
VL_EMBEDDING_ENDPOINT=http://localhost:8000/embed
VL_EMBEDDING_API_KEY=your_vl_api_key_here

# JIT Queue Settings
WORKER_ENABLED=true
JIT_QUEUE_MAX_ATTEMPTS=3
JIT_QUEUE_POLL_INTERVAL_MS=5000
JIT_QUEUE_CLAIM_BATCH_SIZE=1

# Backfill Settings
BACKFILL_CHUNK_SIZE=100
FAIL_FAST=false

# Retrieval Strategy (P5-07A Staged Rollout)
# Options: v3 (champion/default), p5_07a (intervention strategy)
# Default: v3 (safe default-off behavior)
RETRIEVAL_STRATEGY=v3
